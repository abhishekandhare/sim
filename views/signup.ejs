<script type ='text/javascript'>
	var app=angular.module('MtaaS', [])
	 app.controller('MtaaS', ['$scope', '$http', '$window', function ( $scope,$http,$window)
	 {	
		$scope.signup = function (){
				var in_fname = $scope.fname;
				var in_lname = $scope.lname;
				var in_email = $scope.email;
				var in_name = $scope.name;
				var in_password = $scope.password;
				var in_cpassword= $scope.cpassword;
				var in_company = $scope.company;
				var in_supplier = $scope.supplier;
                                        if (in_fname == null || in_fname == "")
	{
		alert("First Name cannot be empty");
		return;
	}
                                        if (in_lname == null || in_lname == "")
	{
		alert("Last Name cannot be empty");
		return;
	}
                                        if (in_email == null || in_email == "")
	{
		alert("Email Id cannot be empty");
		return;
	}
                                        if (in_name == null || in_name == "")
	{
		alert("Username cannot be empty");
		return;
	}
                                        if (in_password == null || in_password == "")
	{
		alert("Password cannot be empty");
		return;
	}
    if (in_password != in_cpassword)
	{
		alert("passwords do not match");
		return;
	}
     if (in_company == null || in_company == "")
	{
		alert("Please enter your Company");
		return;
	}
    if (in_supplier == null || in_supplier == "")
	{
		alert("Please enter Supplier");
		return;
	}
    
				
		$http({
			  method: 'POST',
			  url: '/signup',
			  data: {"fname":in_fname,"lname":in_lname,"email": in_email,"name":in_name,"password":in_password, "cpassword":in_cpassword
		  , "company" : in_company, "supplier":in_supplier}
			}).
			  then(function(response) {

				  $window.location.href = '/login';
			  });
			  
			} 
			
}]);
		</script>
