        <div id="navbar" class="collapse navbar-collapse">
          <ul class="nav navbar-nav">
            <li class="active"><a href="#" ng-click = "home()">Home</a></li>
            <li><a href="#" ng-click = "searchsupplier()">Search</a></li>
            <li><a href="#" ng-click = "processsupplier()">Process</a></li>
          </ul>
           <label class="logoutLblPos">
          <a type="submit" align="right" class="btn btn-success" ng-click="logoutsession()" role="button" >Logout</a>
          </label>
        </div><!-- /.nav-collapse -->
      </div><!-- /.container -->
    </nav><!-- /.navbar -->

    <div class="container">

      <div class="row row-offcanvas row-offcanvas-right">

        <div class="col-xs-12 col-sm-9">
          <p class="pull-right visible-xs">
            <button type="button" class="btn btn-primary btn-xs" data-toggle="offcanvas">Toggle nav</button>
          </p>
          <div class="jumbotron">
              <h2 color="Red"><b>EDIT SUPPLIER</b></h2>
                  
              <!--new code added here -->
              <div class="container well" style="margin-top: 50px">
    <form id="register-form">
        
      <p>Supplier Basic Information</p>
        <a name="sbi"></a>
      <div class="form-group col-md-12">
        <input class="form-control" name="Supplier Name" placeholder="Supplier Name" type="text" disabled>
      </div>
      <div class="form-group col-md-12">
        <input class="form-control" name="Supplier Email Address" placeholder="Supplier Email Address" type="email" disabled>
      </div>
      <div class="form-group col-md-6">
        <Select class="form-control" name="Legal Structure" placeholder="Legal Structure" type="select" disabled>
        <option value="stationery">Select Legal Structure</option>
        <option value="stationery">Stationery</option>
        <option value="saab">Saab</option>
        <option value="mercedes">Mercedes</option>
        <option value="audi">Audi</option>
        </select>
      </div>
        <div class="form-group col-md-6">
        <Select class="form-control" name="Commodity" placeholder="Commodity" type="select" disabled>
        <option value="empty">Select Commodity</option>
        <option value="stationery">Stationery</option>
        <option value="saab">Saab</option>
        <option value="mercedes">Mercedes</option>
        <option value="audi">Audi</option>
        </select>
      </div>
        
      <div class="form-group col-md-12">
        <input class="form-control" name="Company WebSite (URL)" placeholder="Company WebSite (URL)" type="text" disabled>
      </div>
      <div class="form-group col-md-9">
        <Select class="form-control" name="glocation" placeholder="Supplier Geographical Location" type="select" disabled>
        <option value="empty">Select Geographical Location</option>
        <option value="country1">Country 1</option>
        <option value="country2">Country 2</option>
        <option value="country3">Country 3</option>
        <option value="country4">Country 4</option>
        </select>
     </div>
        <a name="sai"></a>
      <div class="clearfix">
          
      </div>
      <p>Supplier Address Information</p>
      <div class="form-group col-md-12">
        <input class="form-control" name="addLine1" placeholder="Address Line 1" type="text">
      </div>
     <div class="form-group col-md-12">
       <input class="form-control" name="addLine2" placeholder="Address Line 2" type="text">
     </div>
    <div class="form-group col-md-6">
       <input class="form-control" name="city" placeholder="City" type="tel">
     </div>

     <div class="form-group col-md-6">
       <input class="form-control" name="postCode" placeholder="Post Code" type="text">
     </div>
     <div class="form-group col-md-6">
        <Select class="form-control" name="country" placeholder="Country" type="select">
        <option value="empty">Select Country</option>
        <option value="country1">Country 1</option>
        <option value="country2">Country 2</option>
        <option value="country3">Country 3</option>
        <option value="country4">Country 4</option>
        </select>
     </div>
        <div class="form-group col-md-6">
        <Select class="form-control" name="state" placeholder="State" type="select">
        <option value="empty">Select State</option>
        <option value="state1">State 1</option>
        <option value="state2">State 2</option>
        <option value="state3">State 3</option>
        <option value="state4">State 4</option>
        </select>
      </div>
        
    <a name="sci"></a>
      <div class="clearfix">
          
      </div>
<!-- Supplier Contact Information -->
    <p>Supplier Primary Contact Information</p>
      <div class="form-group col-md-6">
        <input class="form-control" name="fname" placeholder="First Name" type="text">
      </div>
     <div class="form-group col-md-6">
       <input class="form-control" name="lname" placeholder="Last Name" type="text">
     </div>
    <div class="form-group col-md-12">
       <input class="form-control" name="phone" placeholder="Phone Number" type="tel">
     </div>

     <div class="form-group col-md-12">
       <input class="form-control" name="fax" placeholder="Fax Number" type="text">
     </div>
     <div class="form-group col-md-12">
       <input class="form-control" name="pemail" placeholder="Primary Contact Email Address" type="text">
     </div>
<!-- Supplier Contact Information Ends Here -->
        
     <div class="form-group col-md-12">
       <div class="checkbox">
         <label>
           <input id="terms" name="terms" type="checkbox"> 
           I have read, consent and agree to Company's User Agreement and Privacy Policy
           (including the processing and disclosing of my personal data), and I am of 
           legal age. I understand that I can change my communication preferences at any
           time. Please read the Key Payment and ServiceInformation.
         </label>
       </div>
     </div>
  </form>
  </div>
              <!--new code ends here-->
            
            <a type="submit" class="btn btn-success" href="#" ng-click = "searchsupplier()" role="button">Submit</a><p></p>
              
          </div>
          
        </div><!--/.col-xs-12.col-sm-9-->

        
